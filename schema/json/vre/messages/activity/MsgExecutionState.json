{
  "$schema": "http://json-schema.org/schema",
  "$id": "https://dharpa.org/schemas/vre/messages/activity/MsgExecutionState.json",
  "type": "object",
  "description": "Announces current state of the backend. Useful for letting the user know if they need to wait.",
  "additionalProperties": false,
  "properties": {
    "state": {
      "type": "string",
      "description": "Current state.",
      "enum": [
        "busy",
        "idle"
      ]
    }
  },
  "required": [
    "state"
  ]
}